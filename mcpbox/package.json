{
  "name": "mcpbox",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "serve": "npm run build && firebase emulators:start",
    "gen:strapi": "env-cmd graphql-codegen --config ./strapi.codegen.ts",
    "gen:github": "yarn openapi:github && npx swagger-typescript-api generate --path ./src/_services/github/openapi.json -o ./src/_services/github/__generated__ --axios --single-http-client --modular --clean-output --extract-request-body --extract-request-params --extract-response-body --extract-response-error --unwrap-response-data --single-http-client",
    "openapi:github": "curl -o src/_services/github/openapi.json https://raw.githubusercontent.com/github/rest-api-description/main/descriptions-next/ghes-3.17/ghes-3.17.json -H 'Accept: application/json'"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/platform-express": "^11.0.1",
    "@octokit/rest": "^22.0.0",
    "@types/lodash": "^4.17.20",
    "axios": "^1.10.0",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.3.2",
    "graphql": "^16.11.0",
    "lodash": "^4.17.21",
    "nestjs-octokit": "^0.2.0",
    "octokit": "^5.0.3",
    "react": "^19.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript-document-nodes": "^4.0.16",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "env-cmd": "^10.1.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "swagger-typescript-api": "^13.2.7",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
